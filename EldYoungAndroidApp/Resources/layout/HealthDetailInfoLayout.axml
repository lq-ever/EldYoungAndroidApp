                    android:layout_height="50dp"
                    android:layout_marginTop="1dp"
                    android:background="@color/white"
                    android:orientation="horizontal"
                    android:gravity="center_vertical">
                    <TextView
                        android:id="@+id/tv_BodyTemperature"
                        android:layout_height="match_parent"
                        android:layout_width="wrap_content"
                        android:text="26.7"
                        android:textColor="@color/blue"
                        android:gravity="center"
                        android:textSize="20sp" />
                    <LinearLayout
                        android:layout_height="match_parent"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:layout_marginLeft="5dp">
                        <TextView
                            android:id="@+id/tv_BodyTemperature_High"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="37"
                            android:textColor="@color/lightgray" />
                        <ImageView
                            android:id="@+id/img_BodyTemperature_Arraw"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/uparrow" />
                        <TextView
                            android:id="@+id/tv_BodyTemperature_Low"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="35"
                            android:textColor="@color/lightgray" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    <!--血压 -->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="3dp"
            android:layout_marginRight="3dp">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:gravity="center_vertical"
                android:id="@+id/tv_BloodPressure_Show"
                android:background="@color/blue"
                android:text="血压(mmHg)"
                android:textColor="@color/white" />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="1dp"
                android:background="@color/white"
                android:orientation="horizontal"
                android:gravity="center_vertical">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="高压:"
                    android:textColor="@color/darkgray"
                    android:gravity="center"
                    android:textSize="15sp" />
                <TextView
                    android:id="@+id/tv_BloodPressureParaTwo"
                    android:layout_height="match_parent"
                    android:layout_width="wrap_content"
                    android:text="129"
                    android:textColor="@color/blue"
                    android:gravity="center"
                    android:textSize="20sp" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="低压:"
                    android:textColor="@color/darkgray"
                    android:gravity="center"
                    android:textSize="15sp"
                    android:layout_marginLeft="20dp" />
                <TextView
                    android:id="@+id/tv_BloodPressureParaOne"
                    android:layout_height="match_parent"
                    android:layout_width="wrap_content"
                    android:text="83"
                    android:textColor="@color/blue"
                    android:gravity="center"
                    android:textSize="20sp" />
            <!--                <LinearLayout
                    android:layout_height="match_parent"
                    android:layout_width="50dp"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:layout_marginLeft="25dp">
                    <TextView
                        android:id="@+id/tv_BloodPressure_High"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="23.9"
                        android:textColor="@color/lightgray" />
                    <ImageView
                        android:id="@+id/img_BloodPressure_Arraw"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/uparrow" />
                    <TextView
                        android:id="@+id/tv_BloodPressure_Low"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="18.5"
                        android:textColor="@color/lightgray" />
                </LinearLayout>-->
            </LinearLayout>
        </LinearLayout>
    <!--血脂 -->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="3dp"
            android:layout_marginRight="3dp">
            <TextView
