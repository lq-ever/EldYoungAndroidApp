                            android:layout_height="wrap_content"
                            android:src="@drawable/uparrow" />
                        <TextView
                            android:id="@+id/tv_HeartRate_Low"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="18.5"
                            android:textColor="@color/lightgray" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
        <!--肺活量 -->
            <LinearLayout
